{
  "name": "express-gallery",
  "version": "1.0.0",
  "description": "DevLeague project using Express, Sequelize, HTML5, stored on PostgreSQL",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon server.js",
    "build-sass": "node-sass sass/styles.scss public/css/styles.css",
    "watch-sass": "nodemon -e scss -w sass -x 'npm run build-sass'",
    "build": "npm run build-sass",
    "watch": "npm run watch-sass"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evermuse/express-gallery.git"
  },
  "keywords": [
    "express",
    "sequalize",
    "html5",
    "postgresSQL"
  ],
  "author": "bryan alexander",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/evermuse/express-gallery/issues"
  },
  "homepage": "https://github.com/evermuse/express-gallery#readme",
  "dependencies": {
    "express": "^4.13.3",
    "jade": "^1.11.0",
    "pg": "^4.4.3",
    "pg-hstore": "^2.3.2"
  },
  "devDependencies": {
    "connect-livereload": "^0.5.3",
    "gulp": "^3.9.0",
    "gulp-connect": "^2.2.0",
    "gulp-livereload": "^3.8.1",
    "gulp-nodemon": "^2.0.4",
    "gulp-sass": "^2.0.4",
    "node-sass": "^3.3.3",
    "sequelize": "^3.12.2"
  }
}
